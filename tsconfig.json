{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "declaration": true,
    "sourceMap": true,
    "outDir": "."
  },
  "exclude": [
    "node_modules",
    "typings"
  ]
}
